{
  "name": "lp-model",
  "version": "0.4.2",
  "description": "JavaScript package for modelling (Integer) Linear Programs",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "npm run build && mocha",
    "parser": "peggy -o src/parse-lp-format.js --format es src/parse-lp-format.peggy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DominikPeters/lp-model.git"
  },
  "main": "dist/lp-model.js",
  "unpkg": "dist/lp-model.js",
  "jsdelivr": "dist/lp-model.js",
  "browser": "dist/lp-model.js",
  "keywords": [
    "ilp",
    "mip",
    "lp",
