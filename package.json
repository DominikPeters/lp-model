  "bugs": {
    "url": "https://github.com/DominikPeters/lp-model/issues"
  },
  "homepage": "https://github.com/DominikPeters/lp-model",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "chai": "^5.1.0",
    "glpk.js": "^4.0.2",
    "highs": "^1.0.1",
    "javascript-lp-solver": "^0.4.24",
    "mocha": "^10.3.0",
    "peggy": "^4.0.0",
    "rollup": "^4.12.0"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist"
